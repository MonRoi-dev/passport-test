generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            String      @id @default(uuid())
  email         String      @db.VarChar(32) @unique
  password      String      @db.VarChar(64)
  refreshToken  String
}